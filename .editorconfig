# http://editorconfig.org

root = true


[*]
indent_style = tab
indent_size = 8
tab_width = 8
end_of_line = lf
charset = utf-8
#trim_trailing_whitespace = true
insert_final_newline = true


[*.{c,h}]
indent_size = 2

[*.py]
indent_style = space
indent_size = 4
charset = utf-8

[{catweasl.c,*.man}]
indent_size = 4
